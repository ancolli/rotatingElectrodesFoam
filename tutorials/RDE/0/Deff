FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Deff;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 5e-10;

boundaryField
{
    
    axis
    {
	type            empty;    
    }
    "wedge.*"
    {
        type            wedge;
    }
    "wall.*"
    {
        type            zeroGradient;
    }

}

